@namespace Blazor.WinOld.Components

@inherits WinOldComponentBase

@if (ParentTabs is not null && ParentTabs.SelectedTabPanel == this)
{
    <article role="tabpanel"
             class="panel-win @GetComponentClass() @Class"
             style="@Style"
             disabled="@Disabled"
             @attributes="Attributes">
        @ChildContent
    </article>
}
